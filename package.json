{
  "name": "@relekang/cli-editor",
  "version": "0.0.0",
  "description": " An wrapper around the default terminal editor that can edit any object.",
  "main": "dist/index.js",
  "repository": "git@github.com:relekang/cli-editor.git",
  "author": "Rolf Erik Lekang <me@rolflekang.com>",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "yarn build && yarn flow-gen",
    "build": "babel -d dist src",
    "flow-gen": "flow gen-flow-files src/index.js > dist/index.js.flow"
  },
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": 8
          }
        }
      ],
      "@babel/flow"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.5.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-prettier": "^2.6.2",
    "flow-bin": "^0.80.0",
    "prettier": "^1.14.2"
  },
  "dependencies": {
    "js-yaml": "^3.12.0",
    "lodash.omit": "^4.5.0",
    "mkdirp": "^0.5.1"
  }
}
